# Application
NODE_ENV=development
PORT=3000
API_PREFIX=api/v1

# Database (será configurado no próximo passo com Prisma)
# Nota: Usando porta 54321 para evitar conflito com PostgreSQL local
DATABASE_URL=postgresql://postgres:postgres@localhost:54321/portal_locadora?schema=public

# JWT (será configurado em autenticação)
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRATION=7d

# CORS
CORS_ORIGIN=http://localhost:5173

# Logs
LOG_LEVEL=debug
